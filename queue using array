#include <stdio.h>
int size=4,queue[10],i;
int rear=-1,front=-1;
void enqueue(int num)
{
    if((rear+1)%size)==front)
    {
        printf("queue is full");
    }
    else
    {
        if(front==-1)
        {
            front=0;
        }
        rear=(rear+1)%size;
        queue[rear]=num;
    }
}

}
void dequeue()
{
    if(front==-1)
    {
        printf("queue is empty");
    }
    if(front==rear)
    {
        front=rear=-1;
    }

    else
    {
        front=(front+1)%size;
    }
}
void display()
{
    int i;
    if(rear==-1)
    {
        printf("queue is empty");

    }
    else
    {
        for(i=front;;i=(i+1)%size)
        {
            printf("%d ",queue[i]);
        }
        if(i==rear)
        {

            break;
        }

    }

}
int main()
{
    enqueue(10);
    enqueue(20);
    enqueue(30);
    enqueue(40);
    dequeue();
    display();

}
